{
  "name": "qualitier",
  "version": "0.1.0",
  "description": "Audit project standards.",
  "bin": "bin/cli.js",
  "main": "index.js",
  "module": "src/qualitier.js",
  "scripts": {
    "format": "prettier --write '*/**/*.js'",
    "test": "test"
  },
  "repository": "prichodko/qualitier",
  "author": "Pavel Prichodko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prichodko/qualitier/issues"
  },
  "homepage": "https://github.com/prichodko/qualitier#readme",
  "dependencies": {
    "chalk": "^2.4.1",
    "cosmiconfig": "^5.0.7",
    "esm": "^3.0.84",
    "jest-diff": "^23.6.0",
    "log-symbols": "^2.2.0",
    "meow": "^5.0.0",
    "please-upgrade-node": "^3.1.1",
    "update-notifier": "^2.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "prettier": "^1.15.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "*.{json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
